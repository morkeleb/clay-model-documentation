{
    "name": "mymodel",
    "generators": [
      "clay-model-documentation"
    ],
    "mixins": [
      {
        "type":"function",
        "name":"has_created",
        "function":"(piece)=>piece.events.push({'name': piece.name+'created'})"
      }
    ],
    "model": {
      "types":[
        {
          "name": "order",
          "mixin": ["has_created"],
          "commands": [
            {
              "name": "finish_order",
              "raise": "order_finished",
              "parameters": [
                {
                  "name": "finished"
                }
              ]
            }
          ],
          "events": [],
          "fields": [
            {
              "name": "test"
            }
          ],
          "relations":["product"]
        },
        {
          "name": "product",
          "mixin": ["has_created"],
          "events": []
        }
      ],
      "actors": [{"name":"supplier"},{"name":"customer"},{"name":"admin"}],
      "stories":[{
        "name":"myfirststory",
        "steps": [
          {
            "actor":"supplier",
            "action":"creates",
            "entity":"product"
          },
          {
            "actor":"supplier",
            "action":"adds",
            "entity":"stock"
          },
          {
            "actor":"customer",
            "action":"creates",
            "entity":"order"
          },
          {
            "actor":"customer",
            "action":"adds",
            "entity":"product",
            "target_entity": "order"
          }
        ]
        }
      ]
    }
  }
  